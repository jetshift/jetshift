{
  "name": "Multi-Project Dev Container",
  "image": "mcr.microsoft.com/devcontainers/python:3.9",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    },
    "jetbrains": {
      "projector": {
        "ide": "PyCharm",
        "version": "2024.1"
      }
    }
  },
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "remoteUser": "vscode",
  "mounts": [
    "source=E:\\Python\\jetshift,target=/workspace/jetshift,type=bind,consistency=cached",
    "source=E:\\Python\\jetshift-core,target=/workspace/jetshift-core,type=bind,consistency=cached"
  ],
  "workspaceFolders": [
    "/workspace/jetshift",
    "/workspace/jetshift-core"
  ]
}
