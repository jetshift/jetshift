{
  "name": "JetShift Python 3.12 Dev Container",
  "image": "jetshift/dc-python-312",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    },
    "jetbrains": {
      "projector": {
        "ide": "PyCharm",
        "version": "2024.1"
      }
    }
  },
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "remoteUser": "jsuser",
  "mounts": [
    "source=E:\\Python\\jetshift-core,target=/workspaces/jetshift-core,type=bind,consistency=cached"
  ],
  "workspaceFolders": [
    "/workspaces/jetshift-core"
  ]
}
